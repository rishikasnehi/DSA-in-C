
#include <stdio.h>

int main()
{
    
    char str1[20], str2[20];
    printf("Enter two strings : \n");
    scanf("%s" "%s",str1,str2);
    int i,j;
    for(i = 0; str1[i] != '\0'; i++)
    {
    }
    for(j = 0; str2[j] != '\0'; j++)
    {
        str1[i] = str2[j];
        i++;
    }
    str1[i] = '\0';
    printf("Concatenated string %s ",str1);
	return 0;
}
